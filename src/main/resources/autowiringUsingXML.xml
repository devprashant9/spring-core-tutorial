<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.example.springCore.dependencyInjection.autowiring.usingXML.Student" name="student">
        <property name="name" value="Prashant Kumar Jha" />
        <property name="age" value="23" />
        <property name="course" value="JavaScript" />
    </bean>

    <bean class="org.example.springCore.dependencyInjection.autowiring.usingXML.GetStudentDetails" name="getDetails" autowire="byName" />

<!--  Other Ways  -->
<!--    <bean class="org.example.springCore.dependencyInjection.autowiring.usingXML.GetStudentDetails" name="getDetails" autowire="byType" />-->
<!--    <bean class="org.example.springCore.dependencyInjection.autowiring.usingXML.GetStudentDetails" name="getDetails" autowire="constructor" />-->

</beans>