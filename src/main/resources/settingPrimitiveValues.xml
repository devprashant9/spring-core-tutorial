<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- This Method Will Set Values to Student Class -->
    <bean class="org.example.springCore.dependencyInjection.setter.PrimitiveValues.Student" name="student">

        <!-- Way 1: Value as a Tag -->
        <property name="id">
            <value>101</value>
        </property>

        <!-- Way 2: Value as an Attribute -->
        <property name="name" value="Prashant Kumar Jha" />
        <property name="age" value="23" />
    </bean>

    <bean class="org.example.springCore.dependencyInjection.setter.PrimitiveValues.GetDetails" name="getDetails">

    </bean>

</beans>